
# Install new rubies when used rather than just complaining about it missing.
export rvm_install_on_use_flag=1

# Gemset Create on use Flag
# This will create a gemset when the gemset is used if it does not exist
#
export rvm_gemset_create_on_use_flag=1


# Source a .rvmrc file in a directory after changing to it, if it exists.
# To disable this feature, set to 0
#
export rvm_project_rvmrc=1


# Automatically trust project .rvmrc files
#
#  export rvm_trust_rvmrcs_flag=1

# Use Ruby 2.3.0
rvm use 2.3.0@demoapp
bundle
